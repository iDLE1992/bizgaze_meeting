            return console.error("Join Meeting Failed.", err.toString());
        });
    };
    BizGazeMeeting.prototype.leaveMeeting = function () {
        this.Log("leaving Meeting " + this.connectedToServer);
        if (this.connectedToServer) {
            this.connection.invoke("LeaveRoom").catch(function (err) {
                return console.error("Leave Meeting Failed.", err.toString());
            });
        }
        else {
            this.stopAllMediaStreams();
            $("form#return").submit();
        }
