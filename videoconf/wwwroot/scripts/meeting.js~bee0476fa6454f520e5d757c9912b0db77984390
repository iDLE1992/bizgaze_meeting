    BizGazeMeeting.prototype.registerServerCallbacks = function () {
        var _this = this;
        this.connection.on('joined', function (roomId, clientInfoMsg) {
            _this.RoomId = roomId;
            var myInfoFromServer = JSON.parse(clientInfoMsg);
            _this.myInfo.Id = myInfoFromServer.Id;
            _this.myInfo.Name = myInfoFromServer.Name;
            _this.myInfo.IsHost = myInfoFromServer.IsHost;
            _this.hasRoomJoined = true;
            _this.Log('You have joined in roomid = ' + roomId);
            _this.m_UI.updatePanelOnClientInfo(_this.localVideoElem, _this.myInfo, _this.myInfo);
            _this.connectToJitsiServer();
        });
        this.connection.on('error', function (message) {
            alert(message);
        });
        this.connection.on('roomUserList', function (msg) {
            var clients = JSON.parse(msg);
            for (var i = 0; i < clients.length; i++) {
                var client = clients[i];
                if (client.Id == _this.myInfo.Id)
                    continue;
                _this.sendSignalingMessage(client.Id, {
                    type: _this.bizgazePeerLinkOffer,
                    Id: _this.myInfo.Id,
                    peerInfo: _this.myInfo
                });
            }
        });
        this.connection.on('left', function (clientId) {
            if (_this.m_userList.has(clientId))
                _this.Log(_this.m_userList.get(clientId).Name + " has left");
            if (_this.m_userList.has(clientId)) {
                var conn = _this.connMap.get(clientId);
                if (conn != null)
                    conn.closePeerConnection();
                _this.m_userList.delete(clientId);
                _this.connMap.delete(clientId);
            }
            //self leave
            if (clientId == _this.myInfo.Id) {
                _this.stopAllMediaStreams();
                _this.m_userList.clear();
                _this.connMap.clear();
                $("form#return").submit();
            }
        });
        this.connection.on('bye', function () {
            // If peer did not create the room, re-enter to be creator.
            _this.Log("Other peer left room " + _this.RoomId + ".");
        });
        this.connection.on('SignalingMessage', function (sourceId, strMsg) {
            console.log(' received signaling message:', strMsg);
            var msg = JSON.parse(strMsg);
            if (sourceId != _this.myInfo.Id && _this.connMap.has(sourceId)) {
                var peerConn = _this.connMap.get(sourceId);
                peerConn.onSignalingMessage(msg);
            }
        });
        this.connection.on('controlBroadcastMessage', function (strMsg) {
            var msg = JSON.parse(strMsg);
            var targetId = msg.targetId;
            if (msg.type === _this.ControlMessageTypes.GRANT_HOST_ROLE) {
                //this.Log(this.clientMap.get(targetId).Name + " has been set as host");
                if (targetId == _this.myInfo.Id)
                    _this.myInfo.IsHost = true;
                else if (_this.m_userList.has(targetId))
                    _this.m_userList.get(targetId).IsHost = true;
                _this.m_UI.updatePanelOnClientInfo(_this.localVideoElem, _this.myInfo, _this.myInfo);
                _this.connMap.forEach(function (conn, key, map) {
                    conn.updatePanelUI();
                });
            }
            else if (msg.type === _this.ControlMessageTypes.MUTE_AUDIO) {
                if (targetId == _this.myInfo.Id) {
                    _this.myInfo.mediaMute.audioMute = msg.mute;
                    //audio mute/unmute here
                    _this.MuteUnmuteMyAudio();
                    _this.m_UI.updateToolbar(_this.myInfo);
                }
                else if (_this.m_userList.has(targetId)) {
                    _this.m_userList.get(targetId).mediaMute.audioMute = msg.mute;
                }
                _this.m_UI.updatePanelOnClientInfo(_this.localVideoElem, _this.myInfo, _this.myInfo);
                _this.connMap.forEach(function (conn, key, map) {
                    conn.updatePanelUI();
                });
            }
            else if (msg.type === _this.ControlMessageTypes.MUTE_VIDEO) {
                if (targetId == _this.myInfo.Id) {
                    _this.myInfo.mediaMute.videoMute = msg.mute;
                    //video mute/unmute here
                    _this.MuteUnmuteMyVideo();
                    _this.m_UI.updateToolbar(_this.myInfo);
                }
                else if (_this.m_userList.has(targetId)) {
                    _this.m_userList.get(targetId).mediaMute.videoMute = msg.mute;
                }
                _this.m_UI.updatePanelOnClientInfo(_this.localVideoElem, _this.myInfo, _this.myInfo);
                _this.connMap.forEach(function (conn, key, map) {
                    conn.updatePanelUI();
                });
            }
        });
    };
