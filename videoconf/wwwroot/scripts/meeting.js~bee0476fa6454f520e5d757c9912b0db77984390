    BizGazeMeeting.prototype.turnOnScreenShare = function () {
        var _this = this;
        var i = 0;
        while (i < this.localTracks.length) {
            if (this.localTracks[i].getType() === 'video') {
                this.jitsiRoom.removeTrack(this.localTracks[i]);
                this.localTracks[i].dispose();
                this.localTracks.splice(i, 1);
            }
            else {
                ++i;
            }
        }
        this.JitsiMeetJS.createLocalTracks({
            devices: ['desktop']
        })
            .then(function (tracks) {
            if (tracks.length <= 0) {
                _this.turnOnCamera();
                return;
            }
            var screenTrack = tracks[0];
            _this.localTracks.push(screenTrack);
            screenTrack.addEventListener(_this.JitsiMeetJS.events.track.TRACK_MUTE_CHANGED, function () { return _this.Log('screen - mute chagned'); });
            screenTrack.addEventListener(_this.JitsiMeetJS.events.track.LOCAL_TRACK_STOPPED, function () {
                _this.Log('screen - stopped');
                _this.turnOnCamera();
            });
            screenTrack.attach(_this.localVideoElem);
            _this.jitsiRoom.addTrack(screenTrack);
            _this.screenSharing = true;
            _this.m_UI.setScreenShare(true);
        })
            .catch(function (error) {
            _this.turnOnCamera();
        });
    };
